{
  "name": "synthetic-market-research-frontend",
  "version": "0.0.1",
  "description": "SvelteKit frontend for the Synthetic Market Research platform",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "keywords": [
    "sveltekit",
    "cloudflare",
    "ai",
    "market-research"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.46.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/node": "^24.7.2",
    "svelte": "^5.40.0",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.10"
  }
}
